package com.kn.patterns;

public class LeftInverseOfRAT {

	public static void main(String[] args) {
		for(int i=1; i<=5; i++) {
			for (int j=1; j<=5; j++) {
				if(i<=j) {
//					print the star
//					System.out.print("*");
					
//					print i value
					System.out.print(i);
					
//					print column
//					System.out.print(j);
					
//					print Alphabets increasing row
//					System.out.print((char)(i+64));
					
//					print alphabets increasing column
//					System.out.print((char)(j+64));
					
				}else {
					System.out.print(" ");
				}
			}
			System.out.println();
		}
	}

}
